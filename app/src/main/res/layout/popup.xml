<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/popup_tv_bunVenit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/popup_tv_manage"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="166dp"
        android:layout_marginStart="36dp"
        android:text="Bun venit in popup" />

    <TextView
        android:id="@+id/popup_tv_manage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/popup_tv_bunVenit"
        android:layout_marginBottom="127dp"
        android:onClick="manageAccount"
        android:text="Manage Your Account" />

</RelativeLayout>